{
    "name": "pylance-internal",
    "displayName": "Pylance",
    "description": "Pylance language server",
    "version": "0.0.0",
    "private": true,
    "license": "SEE LICENSE IN LICENSE.txt",
    "author": {
        "name": "Microsoft Corporation"
    },
    "publisher": "Microsoft Corporation",
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/pyrx"
    },
    "scripts": {
        "postinstall": "npm run getonnx",
        "getonnx": "node ./build/getonnx.js",
        "clean": "shx rm -rf ./dist ./out",
        "test": "jest --forceExit",
        "test:coverage": "jest --forceExit --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html --coverageReporters=json",
        "posttest:all:pipeline": "npm run combine:coverage",
        "precombine:coverage": "shx mkdir -p reports .nyc_output && shx cp coverage/coverage-final.json reports/coverage-pylance.json && shx cp pyright/server/coverage/coverage-final.json reports/coverage-pyright.json",
        "combine:coverage": "nyc merge reports .nyc_output/out.json && nyc report --reporter html --reporter cobertura  --exclude-after-remap=false --report-dir coverage-merged",
        "createindices": "ts-node -r tsconfig-paths/register ./src/createIndices.ts"
    },
    "dependencies": {
        "assert": "^2.0.0",
        "hash.js": "^1.1.7",
        "uuid": "^8.3.1",
        "vscode-languageserver": "7.0.0-next.7",
        "yauzl": "^2.10.0"
    },
    "optionalDependencies": {
        "onnxruntime": "1.4.0-noopenmp"
    },
    "devDependencies": {
        "@types/jest": "^26.0.15",
        "@types/node": "^12.19.0",
        "@types/tmp": "^0.2.0",
        "@types/uuid": "^8.3.0",
        "@types/yauzl": "^2.9.1",
        "jest": "^26.6.0",
        "jest-extended": "^0.11.5",
        "jest-junit": "^12.0.0",
        "nyc": "^15.1.0",
        "shx": "^0.3.2",
        "tar": "^6.0.5",
        "tmp": "^0.2.1",
        "ts-jest": "^26.4.1",
        "ts-mockito": "^2.6.1",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.0.3"
    }
}
