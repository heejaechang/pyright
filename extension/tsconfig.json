{
	"compilerOptions": {
		"noUnusedLocals": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
        "skipLibCheck": true,
		"target": "es6",
		"module": "commonjs",
		"lib": [ "es2019" ],
		"sourceMap": true,
		"outDir": "out",
		"rootDir": "src",
		"strict": true
	},
	"exclude": [
		"node_modules"
	]
}
