    "version": "0.0.0",
    "license": "Microsoft",
    "author": {
        "name": "Microsoft Corporation"
    },
    "publisher": "ms-python",
    "preview": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/MPLS_NEW"
    },
    "engines": {
        "vscode": "^1.45.0"
    },
    "keywords": [
        "python"
    ],
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/icon.png",
    "main": "./out/extension.js",
    "contributes": {},
    "activationEvents": [
        "onStartupFinished"
    ],
    "scripts": {
        "vscode:prepublish": "cd .. && npm run package && cd extension && npm run compile",
        "build": "tsc",
        "compile": "tsc -p ./",
        "watch": "tsc --watch",
        "eslint": "eslint ./**/*.ts",
        "test": "jest --detectOpenHandles --forceExit",
        "test:pipeline": "jest --detectOpenHandles --forceExit --reporters=jest-junit --reporters=default --coverage --coverageReporters=cobertura --coverageReporters=html --coverageReporters=json"
    },
    "devDependencies": {
        "@types/jest": "^25.2.3",
        "@types/node": "^13.13.12",
        "@types/vscode": "^1.46.0",
        "jest": "^26.0.1",
        "jest-extended": "^0.11.5",
        "jest-junit": "^10.0.0",
        "ts-jest": "^26.1.0",
        "ts-mockito": "^2.6.1",
        "typescript": "^3.9.5",
        "vsce": "^1.76.1"
    }
}
